# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Bright Data Site Unblocker Configuration
BRIGHT_DATA_USERNAME=your_site_unblocker_username
BRIGHT_DATA_PASSWORD=your_site_unblocker_password
BRIGHT_DATA_ENDPOINT=zproxy.lum-superproxy.io:22225

# Bright Data Residential Proxy Configuration (Optional)
BRIGHT_DATA_RESIDENTIAL_USERNAME=your_residential_username
BRIGHT_DATA_RESIDENTIAL_PASSWORD=your_residential_password
BRIGHT_DATA_RESIDENTIAL_ENDPOINT=brd.superproxy.io:22225
BRIGHT_DATA_PROXY_TYPE=site_unblocker

# Google Cloud Platform Configuration
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
GCS_BUCKET_NAME=your-gcs-bucket-name
BQ_DATASET=retail_analytics
BQ_TABLE=products

# Application Configuration
LOG_LEVEL=INFO
BACKOFF_BASE_DELAY=1
BACKOFF_MAX_RETRIES=5
BACKOFF_MAX_WAIT=300
